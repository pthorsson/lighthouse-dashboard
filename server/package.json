{
  "name": "@lhd/server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn build && run-p 'watch:*'",
    "watch:typescript": "tsc -w",
    "watch:nodemon": "nodemon --exec \"node -r dotenv/config\" ../.build/server/server.js --watch ../.build/server --inspect --delay 1 dotenv_config_path=../.env",
    "build": "rm -rf ../.build/server && tsc"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "filesize": "7.0.0",
    "jsonwebtoken": "8.5.1",
    "lighthouse": "9.0.0",
    "lodash": "4.17.21",
    "lru-cache": "6.0.0",
    "module-alias": "2.2.2",
    "mongoose": "5.13.13",
    "node-cron": "3.0.0",
    "node-fetch": "2.6.6",
    "passport": "0.5.0",
    "passport-azure-ad": "4.3.1",
    "passport-jwt": "4.0.0",
    "socket.io": "2.4.1",
    "strip-ansi": "6.0.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.6",
    "@types/lodash": "4.14.177",
    "@types/lru-cache": "5.1.1",
    "@types/module-alias": "2.0.1",
    "@types/node-cron": "2.0.5",
    "@types/node-fetch": "2.5.12",
    "@types/passport": "1.0.7",
    "@types/passport-azure-ad": "4.3.1",
    "@types/passport-jwt": "3.0.6",
    "@types/socket.io": "2.1.13",
    "@types/uuid": "8.3.3",
    "dotenv": "*",
    "esbuild": "0.13.15",
    "nodemon": "2.0.15",
    "typescript": "*"
  }
}
